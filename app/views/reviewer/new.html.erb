<h1>Signup</h1>
<div>
	<%= form_for(:reviewer, url:reviewer_index_path) do |f|%>
	<div class="field">
		<%= f.label :name%>
		<%= f.text_field :name%>
	</div>
	<div class="field">
		<%= f.label :password %>
		<%= f.text_field :password%>
	</div>
	<div class="actions">
		<%= f.submit "Submit"%>
	</div>
	<% end %>
</div>
